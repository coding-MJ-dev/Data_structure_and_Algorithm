{
  "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang++", // Homebrew LLVM Clang 경로
  "C_Cpp.default.includePath": [
    "/opt/homebrew/opt/llvm/include",
    "/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk/usr/include",
    "/Library/Developer/CommandLineTools/usr/include/c++/v1"
  ],
  "files.associations": {
    "*.cpp": "cpp",
    "ostream": "cpp",
    "iostream": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "*.tcc": "cpp",
    "string": "cpp",
    "vector": "cpp",
    "algorithm": "cpp",
    "unordered_map": "cpp",
    "type_traits": "cpp",
    "functional": "cpp",
    "tuple": "cpp",
    "__bit_reference": "cpp",
    "__hash_table": "cpp",
    "__split_buffer": "cpp",
    "deque": "cpp",
    "initializer_list": "cpp",
    "string_view": "cpp",
    "iosfwd": "cpp",
    "__verbose_abort": "cpp",
    "cmath": "cpp",
    "cstddef": "cpp",
    "cstdint": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "limits": "cpp",
    "locale": "cpp",
    "new": "cpp",
    "stdexcept": "cpp",
    "typeinfo": "cpp",
    "__locale": "cpp",
    "cctype": "cpp",
    "clocale": "cpp",
    "cstdarg": "cpp",
    "ctime": "cpp",
    "execution": "cpp",
    "ios": "cpp",
    "mutex": "cpp",
    "optional": "cpp",
    "ratio": "cpp",
    "streambuf": "cpp",
    "variant": "cpp"
  },
  "C_Cpp.default.cppStandard": "c++20",
  "C_Cpp.default.cStandard": "c17",
  "C_Cpp.default.intelliSenseMode": "macos-clang-arm64", // M1 Mac용 IntelliSense
  "C_Cpp.errorSquiggles": "enabled"
}
